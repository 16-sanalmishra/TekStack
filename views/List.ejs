<h2 style="text-align: center;">List Of Users</h2>
<hr><br>
<body>
    <table>
        <tr>
            <th>
                Name
            </th>
            <th>
                TechStack
            </th>
            <th>
                Actions
            </th>
            <th>
                Delete
            </th>
        </tr>
    </table>
    <% userList.forEach(user => { %>
    <tr>
        <th>
            <%= user['name'] %>
        </th>
        <th>
            <%= user['techstack'] %>
        </th>
        <th>
         <a href="/list/<%= user._id%>">Click here</a>
        
        </th>
        <th>
            <a href="/delete/<%= user._id%>">Delete</a>
        </th>
        <br>

    </tr>
 <%  }) %>
</body>